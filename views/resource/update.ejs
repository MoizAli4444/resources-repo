 checkAllBoxes=(e)=>{
           
            var set = $("#kt_datatable").find('td:first-child .checkable');
            var checked = $(e).is(':checked');

            $(set).each(function() {
                if ($("#about").hasClass("opened")) {
                $("#about").animate({right: "-700px"}, 1);
                }
                if (checked) {
                    $(this).prop('checked', true);
                    $(this).closest('tr').addClass('selected');
                    $('.DTFC_Cloned tr').addClass('selected')
                    $('.bodyCheckBox').attr('checked', true);

                }
                else {
                    $(this).prop('checked', false);
                    $(this).closest('tr').removeClass('selected');
                    $('.DTFC_Cloned tr').removeClass('selected');
                    $('.bodyCheckBox').attr('checked', false);

                }
            });
